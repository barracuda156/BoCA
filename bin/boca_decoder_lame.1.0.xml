<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "http://www.bonkenc.org/boca/component.dtd">
<component>
  <name>LAME Command Line Decoder</name>
  <version>1.0</version>
  <id>lamecmd-in</id>
  <type>decoder</type>
  <format>
    <name>MPEG 1 Audio Layer 3</name>
    <extension>mp3</extension>
  </format>
  <external>
    <command>codecs/cmdline/lame.exe</command>
    <arguments>--decode %OPTIONS %INFILE %OUTFILE</arguments>
    <mode>file</mode>
    <outformat>wave</outformat>
  </external>
</component>
