<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "http://www.bonkenc.org/boca/component.dtd">
<component>
  <name>Flake FLAC Encoder</name>
  <version>1.0</version>
  <id>flake-out</id>
  <type>encoder</type>
  <format>
    <name>FLAC Audio Files</name>
    <extension>flac</extension>
  </format>
  <external>
    <command>codecs/cmdline/flake.exe</command>
    <arguments>%OPTIONS %INFILE -o %OUTFILE</arguments>
    <mode>file</mode>
    <informat>wave</informat>
  </external>
</component>
